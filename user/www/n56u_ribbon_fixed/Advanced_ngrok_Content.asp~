<script>







<% login_state_hook(); %>







function initial(){



	var id_menu = 5;
	if(sw_mode == '4')
		id_menu = 3;
	if(!support_ipv6())
		id_menu--;

	show_banner(1);
	show_footer();
	show_menu(5,4,id_menu);

	ngrok_init();

	hideLoading();

}







function applyRule(){



    if(validForm()){



        showLoading();







        document.form.action_mode.value = " Apply ";



        document.form.current_page.value = "/Advanced_ngrok_Content.asp";



        document.form.next_page.value = "";       



        document.form.submit();



    }



}







function validForm(){











    return true;



}







function done_validating(action){



    refreshpage();



}







</script>



